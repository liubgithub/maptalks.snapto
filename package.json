    "eslint": "^4.19.1",
    "eslint-config-maptalks": "^0.3.0",
    "eslint-plugin-mocha": "^5.0.0",
    "gulp": "^3.9.0",
    "gulp-connect": "^5.0.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-expect": "^1.1.3",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "maptalks": "^0.42.1",
    "mocha": "^5.2.0",
    "rollup": "^0.66.1",
