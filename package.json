{
  "name": "maptalks.snapto",
  "version": "0.1.11",
  "description": "A snap tool used for mouse point to adsorb geometries on map",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/liubgithub/maptalks.snapto"
  },
  "main": "dist/maptalks.snapto.js",
  "scripts": {
    "dev": "rollup -w -c rollup.config.js",
